{
    "globals": {
      "browser": true
    },
    "extends": ["airbnb-base",  "plugin:cypress/recommended"],
    "env": {
        "node": true,
        "jest": true,
        "browser": true,
        "jquery": true
    },
    "rules": {

        "comma-dangle": 1,
        "indent": ["error", 4],
        "no-unused-vars":"off",
        "linebreak-style":"off",
        "no-console":"off",
        "max-len": "off"
      }
  }
